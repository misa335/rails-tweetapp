<div class="main users-new">
    <div class="container">
        <div class="form-heading">User Registration</div>
        <div class="form users-form">
            <div class="form-body">
                <%= form_tag("/users/create") do %>
                    <p>Username</p>
                    <input name="name">
                    
                    <p>Email Address</p>
                    <input name="email">
                    
                    <input type="submit" value="Sign Up">
                <% end %>
            </div>
        </div>
    </div>
</div>